 "raw_recipe": "{% set version = \"8.0.2\" %}\n{% set xy = version.rsplit('.', 1)[0] %}\n{% set build = 2 %}\n\n{% if not mpi %}\n# conda-smithy misbehaves if mpi is unset\n{% set mpi = 'nompi' %}\n{% endif %}\n\n{% if mpi == 'nompi' %}\n# prioritize nompi variant via build number\n{% set build = build + 100 %}\n{% endif %}\n\npackage:\n  name: neuron\n  version: {{ version }}\n\nsource:\n  - url: https://github.com/neuronsimulator/nrn/releases/download/{{ version }}/full-src-package-{{ version }}.tar.gz\n    sha256: d9d0e401832e6b6f6f35e24d4ce32d2f6ed0e46768ebab92db17ffe8bc978838\n    patches:\n      - x-sentinel.patch\n\nbuild:\n  number: {{ build }}\n  skip: true  # [win]\n\n  # add build string so packages can depend on\n  # mpi or nompi variants explicitly:\n  # `pkg * mpi_mpich_*` for mpich\n  # `pkg * mpi_*` for any mpi\n  # `pkg * nompi_*` for no mpi\n  {% if mpi == \"nompi\" %}\n  {% set mpi_prefix = \"nompi\" %}\n  {% else %}\n  {% set mpi_prefix = \"mpi_\" + mpi %}\n  {% endif %}\n  string: {{ mpi_prefix }}_py{{ py }}h{{ PKG_HASH }}_{{ build }}\n\n  run_exports:\n    {% if mpi == \"nompi\" %}\n    - {{ pin_subpackage(\"neuron\") }}\n    {% else %}\n    - {{ pin_subpackage(\"neuron\") }} {{ mpi_prefix }}_*\n    {% endif %}\n\nrequirements:\n  build:\n    - {{ compiler('c') }}\n    - {{ compiler('cxx') }}\n    - make  # [linux]\n    - cmake\n    - bison\n    - flex\n    - pkg-config\n    - libtool\n  host:\n    - python\n    - cython\n    - numpy\n    - readline\n    - ncurses\n    - xorg-libx11\n    - xorg-libxcomposite\n    - xorg-libxext\n    - {{ mpi }}  # [mpi != \"nompi\"]\n  run:\n    - python\n    - numpy\n    - readline\n    - ncurses\n    - {{ pin_compatible('xorg-libx11') }}\n    - {{ pin_compatible('xorg-libxcomposite') }}\n    - {{ pin_compatible('xorg-libxext') }}\n    - {{ mpi }}  # [mpi != \"nompi\"]\n    - {{ compiler('c') }}  # [mpi != \"nompi\"]\n    - {{ compiler('cxx') }}  # [mpi != \"nompi\"]\n\ntest:\n  requires:\n    - {{ compiler('c') }}\n    - {{ compiler('cxx') }}\n    - make  # [linux]\n    - scipy\n  files:\n    - pump.mod\n    - test_mpi.py\n  imports:\n    - neuron\n    - neuron.rxd\n\nabout:\n  home: https://neuron.yale.edu\n  license: BSD-3-Clause\n  license_file: Copyright\n  summary: NEURON is a simulator for models of neurons and networks of neuron.\n\nextra:\n  recipe-maintainers:\n    - minrk\n",
