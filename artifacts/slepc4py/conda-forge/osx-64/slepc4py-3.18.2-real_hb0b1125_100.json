 "raw_recipe": "{% set name = \"slepc4py\" %}\n{% set version = \"3.18.2\" %}\n{% set sha256 = \"402297fd8e583ed2618d2cba05e5cae8e9d0a2c3943812a1a138f431ef3479b3\" %}\n{% set build = 0 %}\n\n{% set version_xy = version.rsplit('.', 1)[0] %}\n{% if scalar == \"real\" %}\n{% set build = build + 100 %}\n{% endif %}\n\npackage:\n  name: slepc4py\n  version: {{ version }}\n\nsource:\n  fn: slepc4py-{{ version }}.tar.gz\n  url: https://pypi.io/packages/source/{{ name[0] }}/{{ name }}/{{ name }}-{{ version }}.tar.gz\n  sha256: {{ sha256 }}\n\nbuild:\n  number: {{ build }}\n  script:\n    - export PETSC_DIR=$PREFIX\n    - export SLEPC_DIR=$PREFIX\n    - export OPAL_PREFIX=$PREFIX  # [mpi == 'openmpi']\n    - $PYTHON -m pip -v install --no-deps .\n  skip: true  # [win]\n  string: {{ scalar }}_h{{ PKG_HASH }}_{{ build }}\n  track_features:\n    - slepc4py_complex  # [scalar == \"complex\"]\n\nrequirements:\n  build:\n    - python                                 # [build_platform != target_platform]\n    - cross-python_{{ target_platform }}     # [build_platform != target_platform]\n    - numpy                                  # [build_platform != target_platform]\n    - {{ mpi }}  # [mpi == 'openmpi' and build_platform != target_platform]\n    - {{ compiler('c') }}\n  host:\n    - python\n    - pip\n    - numpy\n    - {{ mpi }}\n    - petsc {{ version_xy }}.* *{{ scalar }}*\n    - slepc {{ version_xy }}.* *{{ scalar }}*\n    - petsc4py {{ version_xy }}.* *{{ scalar }}*\n  run:\n    - python\n    - {{ pin_compatible('numpy') }}\n    - {{ mpi }}\n    - petsc  # pinned by petsc run_exports\n    - slepc  # pinned by slepc run_exports\n    - petsc4py {{ version_xy }}.*  # build pinned by petsc4py->petsc\n  run_constrained:\n    - mpi4py >=3.0.1\n\nabout:\n  home: https://gitlab.com/slepc/slepc\n  summary: Python bindings for SLEPc\n  license: BSD-2-Clause\n  license_file: LICENSE.rst\n\nextra:\n  recipe-maintainers:\n    - dalcinl\n    - joseeroman\n    - minrk\n",
