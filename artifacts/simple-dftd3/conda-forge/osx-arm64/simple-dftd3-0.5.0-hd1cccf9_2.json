 "raw_recipe": "{% set name = \"simple-dftd3\" %}\n{% set version = \"0.5.0\" %}\n\npackage:\n  name: {{ name|lower }}\n  version: {{ version }}\n\nsource:\n  url: https://github.com/awvwgk/{{ name }}/archive/refs/tags/v{{ version }}.tar.gz\n  sha256: 47b2614c911085690075dda5af70391c93d9a43fe067eae15bb6db72eac33aab\n  patches:\n    - wrap.patch\n\nbuild:\n  number: 2\n  script:\n    - set -ex  # [unix]\n    - mv $PREFIX/lib/pkgconfig/blas.pc $SRC_DIR  # [unix]\n    - meson setup _build ${MESON_ARGS:---prefix=${PREFIX} --libdir=lib} --buildtype=release -Dblas=custom -Dblas_libs=blas  # [unix]\n    - meson setup _build --prefix=%PREFIX% --buildtype=release --pkg-config-path=%PREFIX%\\lib\\pkgconfig  # [win]\n    - meson compile -C _build\n    - meson test -C _build --no-rebuild --print-errorlogs  # [build_platform == target_platform]\n    - meson install -C _build --no-rebuild\n    - cat $PREFIX/lib/pkgconfig/s-dftd3.pc  # [unix]\n    - mv $SRC_DIR/blas.pc $PREFIX/lib/pkgconfig  # [unix]\n\n  run_exports:\n    - {{ pin_subpackage(name, max_pin='x.x') }}\n\nrequirements:\n  build:\n    - {{ compiler('c') }}  # [unix]\n    - {{ compiler('fortran') }}  # [unix]\n    - {{ compiler('m2w64_c') }}  # [win]\n    - {{ compiler('m2w64_fortran') }}  # [win]\n    - meson\n    - ninja\n    - asciidoctor  # [unix]\n    - pkg-config\n    - llvm-openmp  # [osx]\n    - libgomp  # [linux]\n  host:\n    - libblas  # [unix]\n    - mctc-lib\n    - toml-f\n  run:\n    - mctc-lib\n    - toml-f\n\ntest:\n  requires:\n    - pkg-config\n  commands:\n    - s-dftd3 --version\n    - test -f $PREFIX/lib/libs-dftd3$SHLIB_EXT  # [unix]\n    - set PKG_CONFIG_PATH=%PREFIX%\\lib\\pkgconfig  # [win]\n    - pkg-config s-dftd3 --exact-version {{ version }}\n\nabout:\n  home: https://github.com/awvwgk/simple-dftd3\n  license: LGPL-3.0-or-later\n  license_family: LGPL\n  license_file:\n    - COPYING\n    - COPYING.LESSER\n  summary: Simple reimplementation of the DFT-D3 model\n  doc_url: https://awvwgk.gith