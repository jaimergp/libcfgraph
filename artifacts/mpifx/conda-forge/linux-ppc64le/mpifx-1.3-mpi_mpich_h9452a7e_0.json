 "raw_recipe": "{% set name = \"mpifx\" %}\n{% set version = \"1.3\" %}\n{% set build = 0 %}\n{% set mpi = mpi or \"nompi\" %}\n\npackage:\n  name: {{ name|lower }}\n  version: {{ version }}\n\nsource:\n  url: https://github.com/dftbplus/{{ name }}/archive/{{ version }}.tar.gz\n  sha256: b22edb940d4aa2233c0231c41de5049fa14b73619ab390c8873f31fd9920c48b\n  patches:\n    - mpifx42.patch\n\nbuild:\n  number: {{ build }}\n  skip: true  # [win]\n\n  {% set mpi_prefix = \"mpi_\" + mpi %}\n  string: {{ mpi_prefix }}_h{{ PKG_HASH }}_{{ build }}\n\n  run_exports:\n    - {{ name }} * {{ mpi_prefix }}_*\n\nrequirements:\n  build:\n    - {{ compiler('c') }}  # [osx]\n    - {{ compiler('fortran') }}\n    - cmake\n    - ninja\n    - fypp\n    - {{ mpi }}  # [mpi == \"openmpi\" and build_platform != target_platform]\n  host:\n    - {{ mpi }}\n  run:\n    - {{ mpi }}\n\ntest:\n  requires:\n    - pkg-config\n  commands:\n    - pkg-config {{ name }} --modversion\n    - test -f $PREFIX/lib/libmpifx${SHLIB_EXT}\n\nabout:\n  home: https://github.com/dftbplus/{{ name }}\n  license: BSD-2-Clause\n  license_family: BSD\n  license_file:\n    - LICENSE\n  summary: Modern Fortran Interface for MPI\n  doc_url: https://dftbplus.github.io/{{ name }}/api/\n\nextra:\n  recipe-maintainers:\n    - aradi\n    - awvwgk\n    - bhourahine\n",
