   "license_file": [
    "/Users/runner/miniforge3/conda-bld/r-rcppalgos_1647504297392/_h_env_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placeho/lib/R/share/licenses/GPL-2"
