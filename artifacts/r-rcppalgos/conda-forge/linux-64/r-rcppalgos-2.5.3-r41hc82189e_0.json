    "/home/conda/feedstock_root/build_artifacts/r-rcppalgos_1648765932207/_h_env_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold_placehold/lib/R/share/licenses/GPL-2"
