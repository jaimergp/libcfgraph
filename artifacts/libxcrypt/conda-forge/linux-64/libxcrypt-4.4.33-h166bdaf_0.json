 "raw_recipe": "{% set name = \"libxcrypt\" %}\n{% set version = \"4.4.33\" %}\n\npackage:\n  name: {{ name|lower }}\n  version: {{ version }}\n\nsource:\n  url: https://github.com/besser82/libxcrypt/releases/download/v{{ version }}/libxcrypt-{{ version }}.tar.xz\n  sha256: e87acf9c652c573a4713d5582159f98f305d56ed5f754ce64f57d4194d6b3a6f\n\nbuild:\n  number: 0\n  skip: true  # [win]\n  run_exports:\n    # The ABI is very stable\n    - {{ pin_subpackage('libxcrypt', max_pin=None) }}\n  script_env:\n    - OBSOLETE_API=no\n  script: $RECIPE_DIR/do_build.sh\n\nrequirements:\n  build:\n    - {{ compiler('c') }}\n    - make\n    - pkg-config\n    # We get perl from yum to avoid creating a dependency cycle\n    # - perl\n\ntest:\n  commands:\n    - test -f \"${PREFIX}/include/crypt.h\"\n    - test -f \"${PREFIX}/lib/libcrypt${SHLIB_EXT}\"\n    - test -f \"${PREFIX}/lib/libcrypt.so.2\"  # [not osx]\n    - test -f \"${PREFIX}/lib/libcrypt.2.dylib\"  # [osx]\n    - test -f \"${PREFIX}/lib/pkgconfig/libxcrypt.pc\"\n    - test ! -f \"${PREFIX}/lib/libcrypt.so.1\"\n    - test ! -f \"${PREFIX}/lib/libcrypt.1.dylib\"\n\noutputs:\n  - name: libxcrypt1\n    script: do_build.sh\n    build:\n      skip: true  # [not linux]\n      script_env:\n        - OBSOLETE_API=glibc\n\n    requirements:\n      build:\n        - {{ compiler('c') }}\n        - make\n        - pkg-config\n        - perl\n\n    test:\n      commands:\n        - test ! -f \"${PREFIX}/include/crypt.h\"\n        - test ! -f \"${PREFIX}/lib/libcrypt${SHLIB_EXT}\"\n        - test ! -f \"${PREFIX}/lib/pkgconfig/libxcrypt.pc\"\n        - test -f \"${PREFIX}/lib/libcrypt.so.1\"\n\n  - name: libxcrypt\n\nabout:\n  home: https://github.com/besser82/libxcrypt\n  license: LGPL-2.1-or-later\n  license_file:\n    - COPYING.LIB\n    - LICENSING\n  summary: Extended crypt library for descrypt, md5crypt, bcrypt, and others\n\nextra:\n  recipe-maintainers:\n    - chrisburr\n",
